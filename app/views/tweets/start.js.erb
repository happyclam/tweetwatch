<% if current_user.serv.status == PREPARED -%>
  $.ajax({
      url: '/tweets/store', 
      data: {},
      dataType: "html"
      async: false
  }).done(function(data, status, xhr) {
      $('#server_status').html("status = 検索タグ:<%= current_user.serv.track %>で集計中");
      $('#server_start').hide(500);
      $('#server_stop').show(500);
  }).fail(function(xhr, status, error) {
      alert('Error Occured(' + error + ' in start.js)');
  });
<% else -%>
  $('#server_status').html("status = not prepared");
  $('#server_stop').show(500);
<% end -%>
